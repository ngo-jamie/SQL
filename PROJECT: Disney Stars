SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Zac Efron';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Cameron Boyce';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Demi Lovato';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Lindsay Lohan';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Nick Jonas';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Miley Cyrus';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Cole Sprouse';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Sabrina Carpenter';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Ross Lynch';

SELECT d.name, t.movie_or_show
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE d.name = 'Debby Ryan';

SELECT *
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE t.movie_or_show LIKE 'Camp Rock%';

SELECT *
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE t.movie_or_show LIKE '%Suite Life%';

SELECT *
FROM disney_stars d
JOIN television t
ON d.id = t.person_id
WHERE t.movie_or_show = 'Jessie';
